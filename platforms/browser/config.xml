<?xml version='1.0' encoding='utf-8'?>
<widget id="com.trulymobilearts.facts" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Facts You Already Know</name>
    <description>Facts you already know is an app that shows you facts you already know.</description>
    <author email="support@trulymobilearts.com" href="http://trulymobilearts.com">Truly Mobile Arts</author>
    <content src="index.html" />
    <icon src="icon.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
